{
  "name": "nx",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "typeorm": "ts-node ./node_modules/typeorm/cli -d apps/api/src/orm.config.ts",
    "migration:sync": "pnpm run typeorm schema:sync",
    "migration:drop": "pnpm run typeorm schema:drop",
    "migration:reset": "pnpm run migration:drop && pnpm run migration:sync",
    "migration:create": "typeorm-ts-node-commonjs migration:create src/migrations/init",
    "migration:generate": "pnpm run typeorm migration:generate --pretty src/migrations/init",
    "migration:revert": "pnpm run typeorm migration:revert",
    "migration:run": "pnpm run typeorm migration:run",
    "migration:show": "pnpm run typeorm migration:show"
  },
  "private": true,
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/typeorm": "^9.0.1",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.0.0",
    "tslib": "^2.3.0",
    "typeorm": "^0.3.10"
  },
  "devDependencies": {
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@nrwl/cli": "14.7.13",
    "@nrwl/eslint-plugin-nx": "14.7.13",
    "@nrwl/jest": "14.7.13",
    "@nrwl/linter": "14.7.13",
    "@nrwl/nest": "14.7.13",
    "@nrwl/node": "14.7.13",
    "@nrwl/workspace": "14.7.13",
    "@types/jest": "28.1.1",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "jest": "28.1.1",
    "jest-environment-jsdom": "28.1.1",
    "nx": "14.7.13",
    "prettier": "^2.6.2",
    "ts-jest": "28.0.5",
    "ts-node": "10.9.1",
    "typescript": "~4.8.2"
  }
}
